{
  "crabaccess": {
    "max_back_query": 7,
    "store": {
      "db": "dynamo"
    },
    "included_sites": ["T0_*", "T1_*", "T2_*"],
    "excluded_sites": ["T1_UK_RAL_ECHO_Disk", "T2_CH_CERNBOX", "T2_MY_UPM_BIRUNI"]
  },
  "globalqueue": {
    "htcondor": {
      "collector": "cmsgwms-collector-global.cern.ch:9620",
      "schedd_constraint": "CMSGWMS_Type =?= \"crabschedd\""
    },
    "store": {
      "db": "dynamo"
    }
  },
  "log_level": "info"
}
